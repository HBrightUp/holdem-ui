{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [ "es2015", "es2017", "es2020", "dom" ],
    "target": "es2020",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "temp/vscode-dist",
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build",
    "settings"
  ]
}